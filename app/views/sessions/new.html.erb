<h1>Login</h1>
<%= render partial: "shared/flash" %>

<%= form_for(:session, url: sessions_path) do |form| %>
  <%= form.label :email, "Email:" %>
  <%= form.text_field :email %>
  <%= form.label :password, "Password:" %>
  <%= form.password_field :password %>
  <%= form.submit "Log In" %>
<% end %>
